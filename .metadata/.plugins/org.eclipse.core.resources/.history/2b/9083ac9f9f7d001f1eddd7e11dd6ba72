package com.springcore.consinj;

public class person {
	private String name;
	private int personid;
	private certi cert;
	
	public person(String name,int personid, certi cert)
	{
		this.name = name;
		this.personid = personid;
		this.cert = cert;
	}

	@Override
	public String toString() {
		return this.name+" : "+this.personid +"{"+this.cert.name+"}";
	}
	
	
	
}
